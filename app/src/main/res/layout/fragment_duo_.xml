<?xml version="1.0" encoding="utf-8"?>
<androidx.core.widget.NestedScrollView xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@drawable/newbackground"
    android:scrollbarThumbVertical="@drawable/scrollbar"
    tools:context=".duo_Fragment">

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:orientation="vertical">

        <TextView
            android:id="@+id/textView7"
            android:layout_width="195dp"
            android:layout_height="24dp"
            android:layout_gravity="center"
            android:layout_marginTop="10sp"
            android:gravity="center"
            android:text="@string/pubg_tournaments"
            android:textColor="#FFFFFF"
            android:textSize="20sp"
            android:textStyle="bold" />

        <TextView
            android:id="@+id/textView15"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginVertical="10dp"
            android:layout_marginStart="15dp"
            android:text="@string/upcoming_matches"
            android:textColor="#FFFFFF"
            android:textSize="15sp" />


        <RelativeLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:gravity="center">


            <ImageView
                android:id="@+id/imageView05"
                android:layout_width="380dp"
                android:layout_height="wrap_content"
                android:background="@drawable/editmatch"
                tools:ignore="ContentDescription" />

            <TextView
                android:id="@+id/textView008"
                android:layout_width="77dp"
                android:layout_height="45dp"
                android:layout_marginStart="280dp"
                android:layout_marginTop="42dp"
                android:layout_marginEnd="22dp"
                android:background="@drawable/rsrectangle"
                android:paddingTop="11sp"
                android:text="@string/rs_10"
                android:textAlignment="center"
                android:textColor="@color/black"
                android:textSize="15sp" />

            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginStart="128dp"
                android:layout_marginTop="50sp"
                android:text="@string/_10_00_am"
                android:textColor="@color/black"
                android:textSize="25sp" />

            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginLeft="18sp"
                android:layout_marginTop="10sp"
                android:text="@string/dual_teamup"
                android:textColor="@color/black"
                android:textSize="20dp" />

            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_alignEnd="@+id/imageView05"
                android:layout_marginTop="100sp"
                android:layout_marginEnd="25dp"
                android:text="5rs/-per kill"
                android:textColor="@color/black" />
        </RelativeLayout>

        <RelativeLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:gravity="center">


            <ImageView
                android:id="@+id/imageView5"
                android:layout_width="380dp"
                android:layout_height="wrap_content"
                android:background="@drawable/editmatch"
                tools:ignore="ContentDescription" />

            <TextView
                android:id="@+id/textView8"
                android:layout_width="77dp"
                android:layout_height="45dp"
                android:layout_marginStart="280dp"
                android:layout_marginTop="42dp"
                android:layout_marginEnd="22dp"
                android:background="@drawable/rsrectangle"
                android:paddingTop="11sp"
                android:text="@string/rs_10"
                android:textAlignment="center"
                android:textColor="@color/black"
                android:textSize="15sp" />

            <TextView
                android:id="@+id/textView14"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginStart="128dp"
                android:layout_marginTop="50sp"
                android:text="@string/_10_00_am"
                android:textColor="@color/black"
                android:textSize="25sp" />

            <TextView
                android:id="@+id/textView19"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginLeft="18sp"
                android:layout_marginTop="10sp"
                android:text="@string/dual_teamup"
                android:textColor="@color/black"
                android:textSize="20dp" />

            <TextView
                android:id="@+id/textView23"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_alignEnd="@+id/imageView5"
                android:layout_marginTop="100sp"
                android:layout_marginEnd="25dp"
                android:text="5rs/-per kill"
                android:textColor="@color/black" />
        </RelativeLayout>
    </LinearLayout>
</androidx.core.widget.NestedScrollView>